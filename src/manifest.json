{
  "background": { "scripts": ["background/index.ts"] },
  "content_scripts": [
    {
      "js": ["content/index.ts"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "web_accessible_resources": ["pages/iframe/index.html"],
  "externally_connectable": {
    "matches": ["http://localhost/*"]
  }
}
